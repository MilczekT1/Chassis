<component name="ProjectRunConfigurationManager">
    <configuration default="false" name="ChassisTestApp" type="SpringBootApplicationConfigurationType"
                   factoryName="Spring Boot" nameIsGenerated="true">
        <option name="ACTIVE_PROFILES" value="local"/>
        <envs>
            <env name="DEPLOYMENT_ENVIRONMENT" value="local"/>
            <env name="OTLP_ENDPOINT_URL" value="{replace}"/>
            <env name="OTLP_PASSWORD" value="{replace}"/>
            <env name="OTLP_USERNAME" value="{replace}"/>
            <env name="SERVICE_NAMESPACE" value="chassis-test-app"/>
        </envs>
        <option name="FRAME_DEACTIVATION_UPDATE_POLICY" value="UnknownPolicyInFreeMode"/>
        <module name="chassis-test-app"/>
        <option name="PROGRAM_PARAMETERS"
                value="-javaagent:agent/grafana-opentelemetry-java.jar \ -Dotel.service.name=${spring.application.name} \ -Dotel.resource.attributes=service.namespace=${SERVICE_NAMESPACE},deployment.environment=${DEPLOYMENT_ENVIRONMENT}"/>
        <option name="SPRING_BOOT_MAIN_CLASS" value="io.github.milczekt1.chassis.testapp.ChassisTestApp"/>
        <extension name="coverage">
            <pattern>
                <option name="PATTERN" value="io.github.milczekt1.chassis.testapp.*"/>
                <option name="ENABLED" value="true"/>
            </pattern>
        </extension>
        <extension name="net.ashald.envfile">
            <option name="IS_ENABLED" value="false"/>
            <option name="IS_SUBST" value="false"/>
            <option name="IS_PATH_MACRO_SUPPORTED" value="false"/>
            <option name="IS_IGNORE_MISSING_FILES" value="false"/>
            <option name="IS_ENABLE_EXPERIMENTAL_INTEGRATIONS" value="false"/>
            <ENTRIES>
                <ENTRY IS_ENABLED="true" PARSER="runconfig" IS_EXECUTABLE="false"/>
            </ENTRIES>
        </extension>
        <method v="2">
            <option name="Make" enabled="true"/>
        </method>
    </configuration>
</component>
